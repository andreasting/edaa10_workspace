
public class Point {

	public Point(int x, int y){
		private int dx = 0;
		private int dy = 0;
		private int x = 0;
		private int y = 0;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	
	public void move (int dx, int dy) {
		x += dx;
		y += dy;
	}
	public String toString() {
		String xcoords = String.valueOf(x);
		String yCoords = String.valueOf(y);
		
	}
	

}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

		
	}


